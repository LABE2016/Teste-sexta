<script id="navigation-handler">
document.addEventListener('DOMContentLoaded', function() {
const tabItems = document.querySelectorAll('.grid-cols-5 > div, .grid-cols-5 > a');
tabItems.forEach((item, index) => {
item.addEventListener('click', function() {
// Remover classe ativa de todos
tabItems.forEach(tab => {
tab.classList.remove('text-primary');
tab.classList.add('text-gray-500');
// Trocar ícones de fill para line
const icon = tab.querySelector('i');
if (icon) {
icon.className = icon.className.replace('-fill', '-line');
}
});
// Adicionar classe ativa ao item clicado
this.classList.remove('text-gray-500');
this.classList.add('text-primary');
// Trocar ícone de line para fill
const icon = this.querySelector('i');
if (icon) {
icon.className = icon.className.replace('-line', '-fill');
}
});
});
});
</script>
<script id="filter-handler">
document.addEventListener('DOMContentLoaded', function() {
// Manipulador para filtros de liga
const leagueButtons = document.querySelectorAll('button.rounded-full');
leagueButtons.forEach(button => {
if (!button.parentElement.classList.contains('flex.overflow-x-auto')) {
button.addEventListener('click', function() {
const parentGroup = this.parentElement.querySelectorAll('button');
parentGroup.forEach(btn => {
btn.classList.remove('bg-primary', 'text-white');
btn.classList.add('bg-gray-100', 'text-gray-600');
});
this.classList.remove('bg-gray-100', 'text-gray-600');
this.classList.add('bg-primary', 'text-white');
});
}
});
});
</script>
<script id="details-handler">
document.addEventListener('DOMContentLoaded', function() {
// Manipulador para expandir/retrair detalhes
const toggleButtons = document.querySelectorAll('.toggle-details');
toggleButtons.forEach(button => {
button.addEventListener('click', function() {
const targetId = this.getAttribute('data-target');
const detailsElement = document.getElementById(targetId);
if (detailsElement.classList.contains('hidden')) {
detailsElement.classList.remove('hidden');
this.innerHTML = 'Ocultar detalhes <i class="ri-arrow-up-s-line ml-1"></i>';
} else {
detailsElement.classList.add('hidden');
this.innerHTML = 'Ver detalhes <i class="ri-arrow-down-s-line ml-1"></i>';
}
});
});
});
</script>
